/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not (in=a, out=Nota);
    Not (in=b, out=Notb);
    And (a=a, b=Notb, out=aNotb);
    And (a=b, b=Nota, out=bNota);
    Or (a=aNotb, b=bNota, out=out);
}